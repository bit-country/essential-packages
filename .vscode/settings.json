{
  // ===
  // Spacing
  // ===
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.quickSuggestionsDelay": 0,
  // Integrate with windowns user
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  // ===
  // Files
  // ===
  "files.exclude": {
    "**/*.log": true,
    "**/*.log*": true
  },
  "search.exclude": {
    "**/dist": true,
    "dist": true,
    "node_modules": true,
    "**/coverge": true,
    "**/node_modules": true,
    "libs": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  // ===
  // Event Triggers
  // ===
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "biomejs.biome",
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": true,
    "quickfix.biome": true,
    "source.fixAll": true,
    "source.addMissingImports": true
  },
  "npm.packageManager": "yarn",
  "cSpell.words": [
    "Apivt",
    "argb",
    "avif",
    "bahmutov",
    "biomejs",
    "bumpp",
    "bundlesize",
    "cacheable",
    "changesets",
    "codacy",
    "codeql",
    "combobox",
    "compat",
    "computeds",
    "decamelize",
    "decamelized",
    "eslintignore",
    "exceljs",
    "execa",
    "fsevents",
    "harrytran",
    "heic",
    "homemage",
    "inmemory",
    "kolorist",
    "kungfutech",
    "lerp",
    "letiables",
    "lozad",
    "Mergeable",
    "Minh",
    "msvideo",
    "npmjs",
    "npmrc",
    "nrwl",
    "nums",
    "nuxi",
    "nuxt",
    "Partytown",
    "patreon",
    "pinia",
    "plusplus",
    "pnpm",
    "pnpx",
    "posva",
    "preactjs",
    "preinstall",
    "pressable",
    "progressbar",
    "quicktime",
    "sarif",
    "shipjs",
    "stylelint",
    "techmely",
    "treeshake",
    "tsup",
    "typedefs",
    "unocss",
    "vite",
    "vitest"
  ]
}
